<div ng-controller="albumController">
<br />
<div class="col-md-2 pull-left">
<ul class="nav nav-pills nav-stacked">
<li><a href="/favourites">Favourites</a></li>
<li><a href="/">Artists</a></li>
<li><a href="/albums">Albums</a></li>
<li><a href="/tracks">Tracks</a></li>
</ul>
</div>
<div class="panel panel-default col-md-10 pull-right" ng-repeat="track in tracks.items">
<div class="panel-body">
<h4 class="album-names col-md-1"> {{track.track_number}}</h4>
<h5 class="album-names col-md-4"> {{track.name}}</h5>
<audio controls class="album-names col-md-4">
<source ng-src={{track.preview_url}} type="audio/mpeg">
</audio>
<h4 class="album-names col-md-1 pull-right btn  btn-primary" role="button" data={{track}}  ng-click="clickAddtoFavouriteTracksEvent($event)" >Add</h4>
</div>
</div>